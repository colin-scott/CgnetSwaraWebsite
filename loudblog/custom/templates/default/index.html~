<script src="navneet/menu_work.js"></script>
<script>

	function modify()
	{
		var tags = document.getElementsByClassName("tags");
		var i;

		for (i = 0; i < tags.length; i++)
		{
			var res = tags[i].innerHTML;
			var res1 = res.split("</a>");
			//alert(res1);
			var len2 = res1.length;
			var j;
			var set = false;
			var con = "";
			var res_con = "";
			for(j = 0; j < len2; j++)
			{
				var res2 = res1[j].split('">')[1];
				var res3 = parseInt(res2);
				var res4 = isNaN(res3);
				if(res4==false)
				{
					set = true;
					res_con = res3;
				}
				else
				{
					con = con+res1[j]+"</a>";
				}
			}
			if(set == false)
			{
			//alert(res);
			}
			else
			{
				var lenx = con.length;
				con = con.substring(0,lenx-4);
con = con+'<a href="http://cgnetswara.org/navneet/index.php?id='+res_con+'" title="All postings with id'+res_con+'">View Original Story</a>';
				tags[i].innerHTML = con;
			}
		}
	}


	modify();
</script>
