<script type="text/javascript">
  $(window).load(function() {
     $('div.report').each(function() {
       im = $(this).find('img.photo');
       para = $(this).find('div.photoright>p');
       if (im.length && para.length) {
         //console.log('foo ' + im.height().toString() + ' and ' + para.height().toString());
         if (im.height() <= para.height()) {
           $(this).find('div.audiobelowphoto').show();
           $(this).find('div.audiobelowtext').hide();
         }
       }
     });
  });
</script>
